{
  "name": "jonquintero/laravel-module-maker",
  "description": "Artisan command to scaffold Laravel modules from custom stubs.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "modules",
    "scaffold",
    "stubs",
    "generator",
    "artisan"
  ],
  "homepage": "https://github.com/jonquintero/laravel-module-maker",
  "support": {
    "issues": "https://github.com/jonquintero/laravel-module-maker/issues",
    "source": "https://github.com/jonquintero/laravel-module-maker"
  },
  "authors": [
    {
      "name": "Jonathan Quintero",
      "email": "jonquintero@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jonquintero\\ModuleMaker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jonquintero\\ModuleMaker\\ModuleMakerServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/filesystem": "^10.0|^11.0|^12.0",
    "doctrine/inflector": "^2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
